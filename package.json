{
  "name": "humane-language",
  "displayName": "Humane Language Support",
  "description": "Syntax highlighting for the Humane programming language.",
  "version": "0.0.1",
  "publisher": "ftruter",
  "repository": {
    "type": "git",
    "url": "https://github.com/ftruter/humane"
  },
  "engines": {
    "vscode": "^1.80.0" 
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "humane",
        "aliases": [
          "Humane",
          "hum"
        ],
        "extensions": [
          ".hum",
          ".humane"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "humane",
        "scopeName": "source.humane",
        "path": "./syntaxes/humane.tmLanguage.json"
      }
    ]
  }
}